{\n                    this.timer--;\n                }\n            };\n        },\n        data(){\n            return {\n                code: '',\n                name: '',\n                ws: null,\n                joined: false,\n                uid: null,\n                started: false,\n                options: [],\n                timer: 0\n            }\n        },\n        methods: {\n            join(){\n                this.ws.send(JSON.stringify({\n                    sender: 'player',\n                    action: 'join',\n                    data: {\n                        name: this.name,\n                        code: this.code\n                    }\n                }));\n            },\n            answer(option){\n                this.ws.send(JSON.stringify({sender:'player', id: this.uid, action:'answer', data: {answer: option}}));\n                this.options = [];\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"},"lineCount":null}},"error":null,"hash":"9675e50dfa7274db4a9630106a838175","cacheData":{"env":{}}}